{
	"compilerOptions": {
		"composite": true,
		"noEmit": true,
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"useDefineForClassFields": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["src"]
}
